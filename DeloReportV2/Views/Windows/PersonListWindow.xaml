<Window x:Class="DeloReportV2.Views.Windows.PersonListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeloReportV2.Views.Windows"
        DataContext="{Binding PersonListViewModel ,Source = {StaticResource Locator}}"
        mc:Ignorable="d"
        Title="PersonListWindow" Height="450" Width="640" ResizeMode="NoResize">
    <DockPanel>
        <ListBox DockPanel.Dock="Top" Height="250" Margin="5"></ListBox>
        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal" Height="25" Margin="5" HorizontalAlignment="Center">
            <Button/>
            <Button/>
        </StackPanel>
        <ListBox DockPanel.Dock="Bottom">
            
        </ListBox>
    </DockPanel>
</Window>
